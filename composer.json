{
	"name": "codekandis/base64-codec",
	"description": "This library provides a codec to encode into and decode from Base64 in an object oriented way.",
	"keywords": [
		"php",
		"base64 codec",
		"base64",
		"codec",
		"encode",
		"decode"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/base64-codec/issues",
		"docs": "https://github.com/codekandis/base64-codec/blob/master/README.md",
		"source": "https://github.com/codekandis/base64-codec/tree/master"
	},
	"require": {
		"php": "^7.3",
		"codekandis/code-message-interpreter": "^1"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"sensiolabs/security-checker": "^6",
		"phpunit/phpunit": "^8"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Base64Codec\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Base64Codec\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": [
			"vendor/bin/security-checker security:check composer.lock"
		]
	}
}
