{
	"name": "codekandis/base64-codec",
	"description": "`codekandis/base64-codec` is a library providing a codec to encode into and decode from Base64 in an object oriented way.",
	"version": "3.0.0",
	"keywords": [
		"php",
		"base64 codec",
		"base64",
		"codec",
		"encoder",
		"decoder"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/base64-codec/issues",
		"docs": "https://github.com/codekandis/base64-codec/blob/master/README.md",
		"source": "https://github.com/codekandis/base64-codec/tree/master"
	},
	"require": {
		"php": ">=8.3",
		"codekandis/types": "^1.0.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^5.0.0",
		"rector/rector": "^1.0.5"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Base64Codec\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Base64Codec\\Build\\": [
				"build/"
			],
			"CodeKandis\\Base64Codec\\Tests\\": [
				"tests/"
			]
		}
	}
}
